<!-- Show Physician/:id -->
<div class="clearfix">
<nav class="physician_navbar">
  <div class="title_physician">
    <h1 style="font-size: 40px;"><%= @physician.title %><br><%= @physician.f_name %><br><%= @physician.l_name %></h1>
    <% @users.each do |user| %>
        <%= user.f_name %>
        <%= user.l_name %>
    <% end %>
  </div>
<br>
<%= link_to "Profile", home_index_path %><br>
<%= link_to "Create.Pill", new_pillbox_path %><br>
<%= link_to "Show.Pillbox", index_pillbox_path %><br>
<%= link_to "Create.Physician", new_physician_path %><br>
<%= link_to "Show.Physicians", index_physician_path %><br>
<%= link_to 'Sign Out', destroy_user_session_path, :method => :delete %><br>
</nav>

  <br>
<div class="create_physician">
  <div class="row_physician">
    <div class="col-md-4">
      <h1>physicians  <i class="fa fa-stethoscope" aria-hidden="true"></i></h1>
      <%= @physician.title %>,
      <%= @physician.f_name %>
      <%= @physician.l_name %><br>
      <strong>Specialty: </strong><%= @physician.specialty %><br>
      <strong>Address: </strong><%= @physician.address_line_one %><%= @physician.address_line_two %><br>
      <%= @physician.city %>, <%= @physician.state %> <%= @physician.zip %><br>
      <%= @physician.country %><br>
      <strong>Phone Number: </strong><%= @physician.phone_number %><br>
      <strong>Email: </strong><%= @physician.email %><br>

  <%= link_to "Edit Physician", edit_physician_path(@physician.id) %><br>
  <%= link_to "Delete Physician", destroy_physician_path, :method => :delete %>
      </div>
    </div>
</div>
