<!-- Show All Physicians -->
<div class="clearfix">
<nav class="physician_navbar">
  <div class="title_physician">
    <h1 style="font-size: 40px;">show physicians</h1>
    <% @users.each do |user| %>
        <%= user.f_name %>
        <%= user.l_name %>
    <% end %>
  </div>
<br>
<%= link_to "Profile", home_index_path %><br>
<%= link_to "Create.Pill", new_pillbox_path %><br>
<%= link_to "Show.Pillbox", index_pillbox_path %><br>
<%= link_to "Create.Physician", new_physician_path %><br>
<%= link_to "Show.Physicians", index_physician_path %><br>
<%= link_to 'Sign Out', destroy_user_session_path, :method => :delete %><br>
</nav>

<div class="big_physician">
  <div class="row_physician">
    <h1>physicians  <i class="fa fa-stethoscope" aria-hidden="true"></i></h1>
<% @physicians.each do |f| %>
<div class="col-md-3">
  <br>
  <%= f.title %>,
  <%= f.f_name %>
  <%= f.l_name %><br>
  <strong>Specialty: </strong><%= f.specialty %><br>
  <strong>Address: </strong><%= f.address_line_one %><%= f.address_line_two %><br>
  <%= f.city %>, <%= f.state %> <%= f.zip %><br>
  <%= f.country %><br>
  <strong>Phone Number: </strong><%= f.phone_number %><br>
  <strong>Email: </strong><%= f.email %><br>

    <%= link_to "Edit Physician", edit_physician_path(f.id) %><br>
    <%= link_to "Delete Physician", destroy_physician_path(f.id), :method => :delete %>
  </div>
<% end %>
  </div>
</div>
